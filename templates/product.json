{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title"
        },
        "caption": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price"
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector"
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true,
            "show_gift_card_recipient": true
          }
        },
        "description": {
          "type": "description"
        },
        "collapsible-row-0": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "What's Included",
            "icon": "check_circle",
            "content": "",
            "page": ""
          }
        },
        "collapsible-row-1": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "How It Works",
            "icon": "help",
            "content": "",
            "page": ""
          }
        },
        "collapsible-row-2": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "FAQ",
            "icon": "help_circle",
            "content": "",
            "page": ""
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "caption",
        "price",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "description",
        "collapsible-row-0",
        "collapsible-row-1",
        "collapsible-row-2",
        "share"
      ],
      "settings": {
        "enable_sticky_info": true,
        "gallery_layout": "stacked",
        "media_size": "large",
        "constrain_to_viewport": true,
        "mobile_thumbnails": "hide",
        "hide_variants": true,
        "enable_video_looping": false,
        "padding_top": 36,
        "padding_bottom": 12
      }
    },
    "image-with-text": {
      "type": "image-with-text",
      "blocks": {
        "image-with-text-0": {
          "type": "heading",
          "settings": {
            "heading": "Transform Your Career Journey",
            "heading_size": "h2"
          }
        },
        "image-with-text-1": {
          "type": "text",
          "settings": {
            "text": "<p>Our career coaching services are designed to help you navigate your professional path with confidence. Whether you're looking to advance in your current role, transition to a new industry, or rediscover your professional purpose, we provide the guidance and tools you need to succeed.<\/p><p>With personalized attention and proven strategies, we'll help you unlock your full potential and achieve your career goals.<\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "image-with-text-0",
        "image-with-text-1"
      ],
      "settings": {
        "height": "medium",
        "desktop_image_width": "medium",
        "layout": "text_first",
        "desktop_content_position": "middle",
        "desktop_content_alignment": "center",
        "content_layout": "no-overlap",
        "color_scheme": "scheme-1",
        "mobile_content_alignment": "center",
        "padding_top": 48,
        "padding_bottom": 48
      }
    },
    "testimonials": {
      "type": "testimonials",
      "settings": {
        "title": "Success Stories",
        "heading_size": "h2",
        "color_scheme": "scheme-1",
        "padding_top": 36,
        "padding_bottom": 36
      }
    },
    "related-products": {
      "type": "related-products",
      "settings": {
        "heading": "More Career Services",
        "heading_size": "h2",
        "columns_desktop": 3,
        "color_scheme": "scheme-1",
        "image_ratio": "square",
        "show_secondary_image": false,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "1",
        "padding_top": 36,
        "padding_bottom": 48
      }
    }
  },
  "order": [
    "main",
    "image-with-text",
    "testimonials",
    "related-products"
  ]
}
